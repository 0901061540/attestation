digraph {
  node [shape=rect];
  subgraph cluster_external {
    label="External Inputs";
    materials;
    entryPoint;
    arguments;
  }
  subgraph cluster_system {
    label="System Inputs";
    reproducibility;
    type;
  }
  BuildExecution [shape=oval];
  {type, entryPoint, arguments, materials} -> recipe;
  {recipe, materials, reproducibility} -> BuildExecution -> subject;
}
